        }else{
    %>
        <meta property="og:image" content="<%= CLOUDINARY_IMAGE_URL_PREFIX %>/w_1200/v1563518065/fayfay-media/common/f1-og-en.jpg"/>
    <% } %>
    <meta property="og:image:alt" content="<%= translations['f1.title']  %>"/>
    <meta property="og:title" content="<%= translations['f1.title'] %>"/>
    <meta property="og:description" content="<%= translations['f1.desc'] %>"/>

    <% include include/gtm.header.ejs %>

    <% include include/css.ejs %>
