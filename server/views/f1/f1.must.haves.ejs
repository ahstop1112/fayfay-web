                                <div class="bottom">
                                    <div class="price">
                                        <% switch (ccy) {
                                        case 'HKD' : %>
                                            HK$ 43<span><%= translations['common.up'] %></span>
                                        <% break;
                                        case 'TWD' : %>
                                            NT$ 163<span><%= translations['common.up'] %></span>
                                        <% break;
                                        case 'VND' : %>
                                            <span>Từ</span> ₫ 125,000
                                        <% break;
                                        case 'USD' : %>
                                            US$ 5.4<span><%= translations['common.up'] %></span>
                                        <% break;
                                        case 'CNY' : %>
                                            ¥ 36<span><%= translations['common.up'] %></span>
                                        <% break;
                                        case 'SGD' : %>
                                            S$ 7.4<span><%= translations['common.up'] %></span>
                                        <% break;
                                        case 'MYR' : %>
                                            RM 23<span><%= translations['common.up'] %></span>
                                        <% break;
                                        } %>

                                    </div>
                                    <div class="details">
                                        <%= translations['f1.details'] %>
                                    </div>
                                </div>
                            </div>
                        </a>
                    </li>
                </ul>
            </div>
        </div>
    </div>
</section>
