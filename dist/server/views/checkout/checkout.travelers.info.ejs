<div class="row">
    <div class="col-md-12">
        <div class="orange-header">
            Travelers Information
        </div>
        <div class="greyborderbox">
            <div class="activity">
                <div class="row">
                    <div class="col-lg-2 col-md-2 col-sm-2">
                        <div class="form-group">
                            <label>Title <span class="required">*</span></label>
                            <select class="selectpicker" >
                                <option selected>Please Select</option>
                                <option>Mr.</option>
                                <option>Mrs.</option>
                                <option>Miss</option>
                            </select>
                        </div>
                    </div>
                    <div class="col-lg-5 col-md-5 col-sm-5">
                        <div class="form-group">
                            <label>Passport First Name <span class="required">*</span></label>
                            <input type="text" class="input-field input-s" placeholder="As shown on passport">
                        </div>
                    </div>
                    <div class="col-lg-5 col-md-5 col-sm-5">
                        <div class="form-group">
                            <label>Passport Family Name <span class="required">*</span></label>
                            <input type="text" class="input-field input-s" placeholder="As shown on passport">
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-lg-2 col-md-4 col-sm-4">
                        <div class="form-group">
                            <label>Country Code <span class="required">*</span></label>
                            <select class="selectpicker" >
                                <option selected>Please select</option>
                                <option>Hong Kong</option>
                                <option>Japan</option>
                            </select>
                        </div>
                    </div>
                    <div class="col-lg-5 col-md-8 col-sm-8">
                        <div class="form-group">
                            <label>Phone <span class="required">*</span></label>
                            <input type="text" class="input-field input-s" placeholder="Phone">
                        </div>
                    </div>
                    <div class="col-lg-5">
                        <div class="form-group">
                            <label>Email Address (your voucher will be sent here) <span class="required">*</span></label>
                            <input type="text" class="input-field input-s" placeholder="Email">
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>